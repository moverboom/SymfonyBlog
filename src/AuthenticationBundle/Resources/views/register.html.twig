{% extends 'base.html.twig' %}

{% block body %}
    <div class="form-group">
        {{ form_start(form) }}
        <div class="form-group">
            {{ form_row(form.username) }}
        </div>
        <div class="form-group">
            {{ form_row(form.email) }}
        </div>
        <div class="form-group">
            {{ form_row(form.plainPassword.first) }}
        </div>
        <div class="form-group">
            {{ form_row(form.plainPassword.second) }}
        </div>
        <div class="form-group">
        <div class="form-group">
            {{ form_row(form.register) }}
        </div>

            {{ form_end(form) }}
        </div>
    </div>
{% endblock %}